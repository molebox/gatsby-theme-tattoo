{
    "name": "gatsby-theme-tattoo",
    "version": "0.0.1",
    "license": "MIT",
    "main": "index.js",
    "scripts": {
        "build": "gatsby build",
        "dev": "gatsby develop",
        "clean": "gatsby clean",
        "pretty-check": "prettier --check \"src/**/*.{ts,tsx,js}\"",
        "pretty": "prettier --write \"src/**/*.{ts,tsx,js}\""
    },
    "husky": {
        "hooks": {
            "pre-push": "yarn pretty",
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "src/**/*.{js,ts,tsx}": [
            "yarn pretty-check",
            "git add"
        ]
    },
    "peerDependencies": {
        "gatsby": "^2.13.23",
        "react": "^16.8.6",
        "react-dom": "^16.8.6"
    },
    "devDependencies": {
        "@types/react": "^16.8.23",
        "@types/react-dom": "^16.8.4",
        "gatsby": "^2.13.23",
        "husky": "^3.0.0",
        "lint-staged": "^9.2.0",
        "prettier": "^1.18.2",
        "react": "^16.8.6",
        "react-dom": "^16.8.6"
    },
    "dependencies": {
        "@emotion/core": "^10.0.14",
        "@emotion/styled": "^10.0.14",
        "@horacioh/gatsby-theme-instagram": "^0.1.6",
        "gatsby-plugin-compile-es6-packages": "^1.1.0",
        "gatsby-plugin-emotion": "^4.1.1",
        "gatsby-plugin-google-fonts": "^1.0.1",
        "gatsby-plugin-page-creator": "^2.1.2",
        "gatsby-plugin-sharp": "^2.2.9",
        "gatsby-plugin-typescript": "^2.1.2",
        "gatsby-source-filesystem": "^2.1.6",
        "gatsby-source-sanity": "^5.0.1",
        "gatsby-transformer-sharp": "^2.2.5"
    }
}
